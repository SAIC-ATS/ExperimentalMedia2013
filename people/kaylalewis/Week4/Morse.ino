  delay(words);
  
  //GOD G
  digitalWrite(ledPin, HIGH);
  delay (dah);
  digitalWrite(ledPin,LOW);
  delay(dit);
  digitalWrite(ledPin, HIGH);
  delay (dah);
  digitalWrite(ledPin,LOW);
  delay(dit);
  digitalWrite(ledPin, HIGH);
  delay (dit);
  digitalWrite(ledPin,LOW);
  delay(dah);
  
  //O
